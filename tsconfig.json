{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "types": ["vite/client"]
  },
  "include": ["src/**/*.js", "src/**/*.jsx", "vite.config.js"],
  "exclude": ["dist", "node_modules"]
}
